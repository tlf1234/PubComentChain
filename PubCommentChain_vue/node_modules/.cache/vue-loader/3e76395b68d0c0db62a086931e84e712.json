e-height: 1.5;\n    }\n  .mname{\n      display: inline-block;\n      font-size:16px;\n      color: #000000;\n      margin-right:.3rem;\n      overflow: hidden;\n      text-overflow: ellipsis;\n      white-space: nowrap;  \n      max-width: 100%;            \n    }\n  .d23{\n    display:inline-block;\n    font-size: 12px;\n    color: #fff;\n    /* border:.1rem solid #afafaf; */\n    background-color: #afafaf;\n    padding: 0 .3rem;\n  }\n  .screen{\n    display:inline-block;\n    font-size: 12px;\n    /* border:.1rem solid #afafaf; */\n    padding: 0 .3rem;\n    color: #afafaf;\n    white-space: nowrap;\n  }\n  .score,.auteur,.performer{\n    color: #666;\n    font-size: 13px;\n    margin-top: .3rem;\n  }\n  .score>span{\n      color: #ff9c1c;\n  }\n  .btnandtxt{\n      display: flex;\n      flex-direction: column;\n      align-items: center;\n      justify-content: center;\n  }\n  .btntyle{\n    width:4.3rem;height: 2.5rem;\n    background: linear-gradient(45deg,#ff47a8,#ff2e62);\n    border: 0;\n    border-radius: 2rem;\n    font-size:12px;\n    color: #ffffff;\n    margin-bottom: .5rem;\n  }\n  .btnandtxt{\n    color: #999;\n    font-size:12px;\n  }\n</style> "]}]}